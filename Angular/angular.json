{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"host": {
			"projectType": "application",
			"schematics": {},
			"root": "projects/host",
			"sourceRoot": "projects/host/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/host",
						"index": "projects/host/src/index.html",
						"main": "projects/host/src/main.ts",
						"polyfills": "projects/host/src/polyfills.ts",
						"tsConfig": "projects/host/tsconfig.app.json",
						"aot": true,
						"assets": [
							"projects/host/src/favicon.ico",
							"projects/host/src/assets"
						],
						"styles": [
							"projects/host/src/styles.css"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/host/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "projects/host/src/environments/environment.ts",
									"with": "projects/host/src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							],
							"extraWebpackConfig": "projects/host/webpack.prod.config.js"
						}
					}
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"options": {
						"browserTarget": "host:build",
						"port": 4200,
						"extraWebpackConfig": "projects/host/webpack.config.js"
					},
					"configurations": {
						"production": {
							"browserTarget": "host:build:production",
							"extraWebpackConfig": "projects/host/webpack.prod.config.js"
						}
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "host:build",
						"extraWebpackConfig": "projects/host/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/host/src/test.ts",
						"polyfills": "projects/host/src/polyfills.ts",
						"tsConfig": "projects/host/tsconfig.spec.json",
						"karmaConfig": "projects/host/karma.conf.js",
						"assets": [
							"projects/host/src/favicon.ico",
							"projects/host/src/assets"
						],
						"styles": [
							"projects/host/src/styles.css"
						],
						"scripts": []
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"projects/host/tsconfig.app.json",
							"projects/host/tsconfig.spec.json",
							"projects/host/e2e/tsconfig.json"
						],
						"exclude": [
							"**/node_modules/**"
						]
					}
				},
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "projects/host/e2e/protractor.conf.js",
						"devServerTarget": "host:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "host:serve:production"
						}
					}
				}
			}
		},
		"mfe1": {
			"projectType": "application",
			"schematics": {},
			"root": "projects/mfe1",
			"sourceRoot": "projects/mfe1/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/mfe1",
						"index": "projects/mfe1/src/index.html",
						"main": "projects/mfe1/src/main.ts",
						"polyfills": "projects/mfe1/src/polyfills.ts",
						"tsConfig": "projects/mfe1/tsconfig.app.json",
						"aot": true,
						"assets": [
							"projects/mfe1/src/favicon.ico",
							"projects/mfe1/src/assets"
						],
						"styles": [
							"projects/mfe1/src/styles.css"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/mfe1/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "projects/mfe1/src/environments/environment.ts",
									"with": "projects/mfe1/src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							],
							"extraWebpackConfig": "projects/mfe1/webpack.prod.config.js"
						}
					}
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"options": {
						"browserTarget": "mfe1:build",
						"port": 5000,
						"extraWebpackConfig": "projects/mfe1/webpack.config.js"
					},
					"configurations": {
						"production": {
							"browserTarget": "mfe1:build:production",
							"extraWebpackConfig": "projects/mfe1/webpack.prod.config.js"
						}
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "mfe1:build",
						"extraWebpackConfig": "projects/mfe1/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/mfe1/src/test.ts",
						"polyfills": "projects/mfe1/src/polyfills.ts",
						"tsConfig": "projects/mfe1/tsconfig.spec.json",
						"karmaConfig": "projects/mfe1/karma.conf.js",
						"assets": [
							"projects/mfe1/src/favicon.ico",
							"projects/mfe1/src/assets"
						],
						"styles": [
							"projects/mfe1/src/styles.css"
						],
						"scripts": []
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"projects/mfe1/tsconfig.app.json",
							"projects/mfe1/tsconfig.spec.json",
							"projects/mfe1/e2e/tsconfig.json"
						],
						"exclude": [
							"**/node_modules/**"
						]
					}
				},
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "projects/mfe1/e2e/protractor.conf.js",
						"devServerTarget": "mfe1:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "mfe1:serve:production"
						}
					}
				}
			}
		},
		"mfe2": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:application": {
					"strict": true
				}
			},
			"root": "projects/mfe2",
			"sourceRoot": "projects/mfe2/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/mfe2",
						"index": "projects/mfe2/src/index.html",
						"main": "projects/mfe2/src/main.ts",
						"polyfills": "projects/mfe2/src/polyfills.ts",
						"tsConfig": "projects/mfe2/tsconfig.app.json",
						"assets": [
							"projects/mfe2/src/favicon.ico",
							"projects/mfe2/src/assets"
						],
						"styles": [
							"projects/mfe2/src/styles.css"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/mfe2/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "projects/mfe2/src/environments/environment.ts",
									"with": "projects/mfe2/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/mfe2/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "mfe2:build:production",
							"extraWebpackConfig": "projects/mfe2/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "mfe2:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 6000,
						"extraWebpackConfig": "projects/mfe2/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "mfe2:build",
						"extraWebpackConfig": "projects/mfe2/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/mfe2/src/test.ts",
						"polyfills": "projects/mfe2/src/polyfills.ts",
						"tsConfig": "projects/mfe2/tsconfig.spec.json",
						"karmaConfig": "projects/mfe2/karma.conf.js",
						"assets": [
							"projects/mfe2/src/favicon.ico",
							"projects/mfe2/src/assets"
						],
						"styles": [
							"projects/mfe2/src/styles.css"
						],
						"scripts": []
					}
				}
			}
		}
	},
	"defaultProject": "host"
}